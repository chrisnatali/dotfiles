"xdotool key --clearmodifiers click 4"
	Alt + Up
"xdotool key --clearmodifiers click 5"
	Alt + Down
"xdotool key mousedown 1"
	Control+Alt + d
"xdotool key mouseup 1"
	Control+Alt + u
"xdotool key mousemove_relative --sync 0 20"
    Control+Alt + Down
"xdotool key mousemove_relative --sync 0 -20"
    Control+Alt + Up
"xdotool key mousemove_relative --sync 20 0"
    Control+Alt + Right
"xdotool key mousemove_relative --sync -- -20 0"
    Control+Alt + Left

# Volume Controls
"wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+ --limit 1.5"
    Alt + equal

"wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%-"
    Alt + minus

"wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle"
    Alt + 0
